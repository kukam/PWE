Set-Cookie: [% cookie_name %]=[% cookie_id %]; domain=[% http_host %]; path=[% http_host_path %]; expires=[% cookie_expire_date %]
Date: [% http_gmt_date %]
Pragma: must-revalidate
Expires: 0
Cache-control: must-revalidate
Content-Encoding: UTF-8
Content-Type: application/csv; charset=UTF-8
Content-disposition: attachment; filename=[% IF csv_filename %][% csv_filename %][% ELSE %]download.csv[% END %]

